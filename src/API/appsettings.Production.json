{
  "ConnectionStrings": {
    "LocationConnection": "Server=(localdb)\\mssqllocaldb;Integrated Security=true;Initial Catalog=LocationSearchDb;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft.EntityFrameworkCore.ChangeTracking": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Microsoft.EntityFrameworkCore.Query": "Error",
        "Microsoft.AspNetCore.Hosting.Internal.WebHost": "Warning",
        "Microsoft.AspNetCore": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\logs\\locationSearch\\locationSearchLog.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "outputTemplate": "{Timestamp:dd/MM/yyyy HH:mm:ss.fff zzz};[{Level:u3}];{SourceContext};{Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
